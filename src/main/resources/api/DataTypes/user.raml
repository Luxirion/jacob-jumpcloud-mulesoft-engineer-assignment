#%RAML 1.0 DataType
type: object
properties:
  username:
      type: string
      required: true
      description: Username
      example: "testuser"
  id:
      type: string
      required: true
      description: Id
      example: "1234"
  name:
      type: string
      required: false
      description: Name
      example: "John Smith"